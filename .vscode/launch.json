    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) test_sql",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test_sql",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],"preLaunchTask":"${defaultBuildTask}"
        },

        {
            "name": "(gdb) Launch ringbuffer",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ringbuffer",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [    {"name":"GST_DEBUG", "value":"3,barrier:6"},//,nmeasource:3,basesrc:3,ptsnormalise:5,split*:5,rtsp*:5"},
                                // {"name":"GST_DEBUG","value":"GST_TRACER:7"},
                                // {"name": "GST_TRACERS", "value": "leaks(stack-traces-flags=full)"},
                                {"name": "GST_DEBUG_FILE", "value": "/workspaces/dashcam/ringbuffer.log"},
                                {"name":"GST_DEBUG_DUMP_DOT_DIR","value": "/workspaces/dashcam/dots"}],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],"preLaunchTask":"${defaultBuildTask}"
        },
        {
            "name": "(gdb) Launch join",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/joiner",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [    {"name":"GST_DEBUG", "value":"3,ptsnormalise:5,split*:5"},//,nmeasource:3,basesrc:3,default:4"},
                                {"name":"GST_DEBUG_FILE", "value":"/workspaces/dashcam/joiner.log"},
                                // {"name":"GST_DEBUG","value":"GST_TRACER:7"},
                                // {"name": "GST_TRACERS", "value": "leaks(stack-traces-flags=full)"},
                                // {"name": "GST_DEBUG_FILE", "value": "/workspaces/dashcam/trace.log"},
                                {"name":"GST_DEBUG_DUMP_DOT_DIR","value": "/workspaces/dashcam/dots"}],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],"preLaunchTask":"${defaultBuildTask}"
        }


    ]
}